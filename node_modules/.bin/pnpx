#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/Portfolio/node_modules/.pnpm/pnpm@9.0.6/node_modules/pnpm/bin/node_modules:/home/runner/Portfolio/node_modules/.pnpm/pnpm@9.0.6/node_modules/pnpm/node_modules:/home/runner/Portfolio/node_modules/.pnpm/pnpm@9.0.6/node_modules:/home/runner/Portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/Portfolio/node_modules/.pnpm/pnpm@9.0.6/node_modules/pnpm/bin/node_modules:/home/runner/Portfolio/node_modules/.pnpm/pnpm@9.0.6/node_modules/pnpm/node_modules:/home/runner/Portfolio/node_modules/.pnpm/pnpm@9.0.6/node_modules:/home/runner/Portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pnpm/bin/pnpx.cjs" "$@"
else
  exec node  "$basedir/../pnpm/bin/pnpx.cjs" "$@"
fi
